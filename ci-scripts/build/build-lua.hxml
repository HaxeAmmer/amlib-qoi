-lib ammer
-cp ../src
-D ammer.buildPath=tmp/build
-D ammer.outputPath=bin/lua/bin-lua
amlib.qoi.Qoi

-D ammer.bake=yes
-D ammer.bake.mainType=amlib.qoi.Qoi
-D ammer.bake.output=bin/lua
-D ammer.bake.rootToBin=bin-lua
-D ammer.bake.downloadURL=https://github.com/HaxeAmmer/amlib-qoi/releases/download/v0.0.1/prebuilt-$osName-lua.bin

-lua tmp/out.lua
